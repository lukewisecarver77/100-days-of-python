row1 = ["a", "b", "c"]
row2 = ["d", "e", "f"]
row3 = ["g", "h", "i"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")

horizontal = int(position[0]) -1
vertical = int(position[1]) -1

map[vertical][horizontal] = "X"

print(f"{row1}\n{row2}\n{row3}")
